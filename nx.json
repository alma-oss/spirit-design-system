{
  "pluginsConfig": {
    "@nrwl/js": {
      "analyzeSourceFiles": false
    }
  },
  "targets": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^build"]
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [],
    "production": ["default"]
  },
  "targetDefaults": {
    "lint": {
      "cache": true
    },
    "test": {
      "cache": true
    },
    "types": {
      "cache": true
    },
    "build": {
      "cache": true
    }
  },
  "neverConnectToCloud": true
}
